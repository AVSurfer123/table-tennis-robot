<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find x6_gazebo)/worlds/gz5_arm.world"/>
    <!-- more default parameters can be changed here -->
  </include>

  <!-- Convert an xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro.py $(find x6_description)/urdf/x6/x6.urdf.xacro" />

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -y 0.2 -z 0.762 -model x6" />

  <include file="$(find x6_control)/launch/x6_control.launch">
  </include>

  <!-- Throttles the rate of Gazebo updates -->
  <node pkg="topic_tools" type="throttle" name="throttle_sensor" args="messages /gazebo/model_states 120" />


<!-- node pkg="optitrack_sim_tools" type="perfect_tracker.py" name="ball_sim"/ -->

<!-- node pkg="optitrack_to_biorob_tf" type="fixed_optitrack_to_biorob_tf_node.py" name="optitrack_to_world_tf_publisher"/ -->


</launch>
